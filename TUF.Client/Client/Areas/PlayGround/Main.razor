@page "/Playground"
@using TUF.Client.Shared.Dtos.OldMarket;
 <TUFTitle Title="번개장터" Description="번개장터" />
<MudContainer Class="mt-2 px-2" MaxWidth="MaxWidth.ExtraExtraLarge">
    <MudGrid xs="12" sm="12" md="12"  Justify="Justify.Center" Elevation="0" >
       <MudItem xs="12" sm="12" md="12" >
            <MudPaper Elevation="2" Class="d-flex align-start flex-grow-1 gap-4 pa-6" Style="height: 100px;">
                <MudStack Row="true" >
                <MudAutocomplete T="string" Label="검색어" @bind-Value="param.Keyword" SearchFunc="@Search1"
                                 ResetValueOnEmptyText="@resetValueOnEmptyText"
                                 Clearable="true" For="@(() => param.Keyword)" Style="width:400px"                                 
                                 CoerceText="@coerceText" CoerceValue="@coerceValue" />
                    <MudButton Variant="Variant.Outlined" Class="pa-3 px-4" Color="Color.Primary">검색하기</MudButton>
              </MudStack>
            </MudPaper>
        </MudItem>       
    </MudGrid>
    
    <MudGrid xs="12" sm="12" md="12" Justify="Justify.Center" >
        <MudItem xs="10" sm="10" md="10">
            <MudPaper Elevation="2" Class="pa-4" Style="height: 1000px;">
                <MudItem xs="3" sm="3" md="3" Justify="Justify.Center">
                    <MudCard Style="background:#054aa1" Class="m-2 ">
                        <MudCardMedia Class="pa-4 " Image="https://image.newdaily.co.kr/site/data/img/2017/06/15/2017061500093_0.jpg" Height="250" Spacing="2" />
                        <MudDivider />
                        <MudCarousel Class="mud-width-full mt-2" Style="height:200px;" AutoCycle="false" TData="object">
                            <BulletTemplate Context="selected">
                                <div Class="mud-button-root mud-icon-button mud-icon-button-color-inherit mud-ripple mud-ripple-icon">
                                    <span class="mud-icon-button-label">
                                        <MudIcon Icon="@(selected ? Icons.Material.Filled.CheckCircle : Icons.Material.Filled.Circle)" Color="@Color.Inherit" />
                                    </span>
                                </div>
                            </BulletTemplate>
                            <PreviousButtonTemplate>
                                <div Class="mud-button-root mud-icon-button mud-icon-button-color-inherit mud-ripple mud-ripple-icon">
                                    <span class="mud-icon-button-label">
                                        <MudIcon Class="mud-ripple mud-ripple-icon mud-icon-button-size-medium" Icon="@Icons.Material.Filled.SkipPrevious" Color="@Color.Inherit" />
                                    </span>
                                </div>
                            </PreviousButtonTemplate>
                            <NextButtonTemplate>
                                <div Class="mud-button-root mud-icon-button mud-icon-button-color-inherit mud-ripple mud-ripple-icon">
                                    <span class="mud-icon-button-label">
                                        <MudIcon Class="mud-ripple mud-ripple-icon mud-icon-button-size-medium" Icon="@Icons.Material.Filled.SkipNext" Color="@Color.Inherit" />
                                    </span>
                                </div>
                            </NextButtonTemplate>
                            <ChildContent>
                                <MudCarouselItem Color="@Color.Primary">
                                    <div class="d-flex" style="height:100%">
                                        <MudImage Src="https://img.wowtv.co.kr/wowtv_news/20170618/B20170618140143327.jpg" Height="250" Width="250" />
                                    </div>
                                </MudCarouselItem>
                                <MudCarouselItem Color="@Color.Secondary">
                                    <div class="d-flex" style="height:100%">
                                        <MudImage Src="http://img.segye.com/content/image/2017/06/15/20170615516929.jpg" Height="250" Width="250" />
                                    </div>
                                </MudCarouselItem>
                                <MudCarouselItem>
                                    <div class="d-flex" style="height:100%">
                                        <MudImage Src="data:image/jpeg;base64," Height="250" Width="250" />
                                    </div>
                                </MudCarouselItem>
                            </ChildContent>
                        </MudCarousel>
                        <MudDivider />
                        <MudCardContent>
                            <MudText Typo="Typo.h5">10,000</MudText>
                            <MudText Typo="Typo.body2">수펑 가아나아다나마</MudText>
                        </MudCardContent>
                        <MudCardActions>
                            <MudButton Variant="Variant.Text" Color="Color.Primary">보기</MudButton>
                        </MudCardActions>
                    </MudCard>
                </MudItem>

            </MudPaper>
        </MudItem>
        <MudItem xs="2" sm="2" md="2">
            <MudPaper Elevation="2" Class="pa-4" >
            </MudPaper>
        </MudItem>
    </MudGrid>
</MudContainer>

 @code
 {
    BungaeDto.Request param = new();
    private string[] states =
      {
        "내셔널지오그래픽", "다이나핏", "스노우피크", "코닥"
    };
    private string value1;
    private bool resetValueOnEmptyText;
    private bool coerceText;
    private bool coerceValue;

    private async Task<IEnumerable<string>> Search1(string value)
    {
        // In real life use an asynchronous function for fetching data from an api.
        await Task.Delay(5);

        // if text is null or empty, show complete list
        if (string.IsNullOrEmpty(value))
            return states;
        return states.Where(x => x.Contains(value, StringComparison.InvariantCultureIgnoreCase));
    }
 }